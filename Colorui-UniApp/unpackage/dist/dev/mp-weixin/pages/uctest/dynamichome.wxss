
.bg-custom {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		        justify-content: flex-end;
		-webkit-box-align: end;
		-webkit-align-items: flex-end;
		        align-items: flex-end;
}
.bg-custom .topImg {
		width: 100%;
		height: 200px;
		/* background-color: #FFFFFF; */
		overflow: hidden;
}
.bg-custom .topImg image {
		width: 100%;
		height: 200px;
		position: fixed;
		top: 155rpx;
		z-index: -1;
		/* 		filter: blur(40px);
		transform: scale(1.2); */
}
.my-detail {
		background-color: #FFFFFF;
		padding-top: 20rpx;
		width: 100%;
		position: relative;
}
.my-detail .cu-avatar {
		position: absolute;
		top: -50rpx;
		left: 30rpx;
}
.fixedBox {
		z-index: 9999;
}

