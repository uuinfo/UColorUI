<view><block wx:if="{{transparent}}"><block><view class="cu-custom"><view class="{{['cu-bar fixed',top<100?'show':'hide']}}" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')+('background-color:'+('rgba(255,255,255,'+(top>100?top>200?1:(top-100)/100:0)+')')+';')}}"><block wx:if="{{isBack}}"><view data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" class="action roundIcon" bindtap="__e"><text class="cuIcon-back"></text></view></block><block wx:if="{{isTitle}}"><view class="action text-bold"><slot name="title"></slot></view></block><view style="opacity:0;">{{''+top+''}}</view><slot name="right"></slot></view></view></block></block><block wx:else><block><view class="cu-custom" style="{{'height:'+(hasHeight?CustomBar+'px':'0px')+';'}}"><view class="{{['cu-bar fixed',bgImage!=''?'none-bg text-white bg-img':'',bgColor]}}" style="{{(style)}}"><block wx:if="{{isBack}}"><view data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text><slot name="backText"></slot></view></block><block wx:if="{{isTitle}}"><view class="action text-bold"><view class="text-lg"><slot name="title"></slot></view></view></block><view class="content" style="{{'top:'+(StatusBar+'px')+';'}}"><slot name="content"></slot></view><slot name="right"></slot></view></view></block></block></view>